# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

#Methods with POST and csrf
POST   /fundacionesC/               controllers.CFundacion.guardarF()
GET    /fundacion/:id               controllers.CFundacion.consultarF(id : java.util.UUID)
GET    /eliminarF/                   controllers.CFundacion.eliminarF(id : java.util.UUID)

# An example controller showing a sample home page
+ nocsrf
GET     /                           controllers.HomeController.index
GET     /inicio/empleado            controllers.HomeController.inicio
GET     /inicio/admin               controllers.HomeController.inicio_admin
GET     /fundaciones/               controllers.CFundacion.fundaciones
GET     /servicios/                 controllers.HomeController.servicios
GET     /solicitudes/               controllers.CSolicitud.solicitudes
GET     /reportes/                  controllers.HomeController.reportes
GET     /fundaciones/incluir        controllers.CFundacion.incl_fundacion
GET     /servicios/incluir          controllers.HomeController.incl_servicio
GET     /reportes/solicitantes      controllers.HomeController.rp_solicitantes
GET     /reportes/presupuesto       controllers.HomeController.rp_presupuesto
GET     /empleados/                 controllers.HomeController.empleados
GET     /empleados/incluir          controllers.HomeController.incl_empleado
GET     /empleados/consultar        controllers.HomeController.consultar_empleado
GET     /reporte/solicitudes        controllers.CSolicitud.rp_solicitudes
GET     /solicitudes/consulta       controllers.CSolicitud.consulta_solicitud

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

